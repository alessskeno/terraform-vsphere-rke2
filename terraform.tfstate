              "\"alertmanager\":\n  \"config\":\n    \"global\":\n      \"resolve_timeout\": \"5m\"\n    \"receivers\":\n    - \"name\": \"slack-network\"\n      \"slack_configs\":\n      - \"actions\":\n        - \"name\": \"dashboardbutton\"\n          \"text\": \"Dashboard :grafana:\"\n          \"type\": \"button\"\n          \"url\": \"https://grafana.hostart.az/d/{{ .CommonLabels.uid }}\"\n        - \"name\": \"mutebutton\"\n          \"text\": \"Silence :no_bell:\"\n          \"type\": \"button\"\n          \"url\": \"{{ template \\\"__alert_silence_link\\\" . }}\"\n        \"api_url\": \"https://hooks.slack.com/services/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\"\n        \"channel\": \"network-notifications\"\n        \"send_resolved\": true\n        \"text\": \"{{ range .Alerts -}}\\r\\n*Severity:* {{ .Annotations.title }}{{ if\n          .Labels.severity }} - `{{ .Labels.severity }}`{{ end }}\\r\\n*Summary:* {{\n          .Annotations.summary }}\\r\\n*Description:* {{ .Annotations.description }}\\r\\n\\r\\n{{\n          end }}\\r\\n\"\n        \"title\": \"{{ define \\\"__alert_severity_prefix_title\\\" -}}\\r\\n    {{ if ne\n          .Status \\\"firing\\\" -}}\\r\\n    :white_check_mark:\\r\\n    {{- else if eq .CommonLabels.severity\n          \\\"critical\\\" -}}\\r\\n    :fire:\\r\\n    {{- else if eq .CommonLabels.severity\n          \\\"warning\\\" -}}\\r\\n    :warning:\\r\\n    {{- else if eq .CommonLabels.severity\n          \\\"info\\\" -}}\\r\\n    :information_source:\\r\\n    {{- else -}}\\r\\n    :question:\\r\\n\n          \\   {{- end }}\\r\\n{{- end }}\\r\\n{{ template \\\"__alert_severity_prefix_title\\\"\n          . }} [{{ .Status | toUpper }}{{ if eq .Status \\\"firing\\\" }} x {{ .Alerts.Firing\n          | len }}{{ end }}] {{ .CommonLabels.alertname }}\\r\\n\"\n    - \"name\": \"slack-host\"\n      \"slack_configs\":\n      - \"actions\":\n        - \"name\": \"dashboardbutton\"\n          \"text\": \"Dashboard :grafana:\"\n          \"type\": \"button\"\n          \"url\": \"https://grafana.hostart.az/d/{{ .CommonLabels.uid }}\"\n        - \"name\": \"mutebutton\"\n          \"text\": \"Silence :no_bell:\"\n          \"type\": \"button\"\n          \"url\": \"{{ template \\\"__alert_silence_link\\\" . }}\"\n        \"api_url\": \"https://hooks.slack.com/services/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\"\n        \"channel\": \"hostart-notifications\"\n        \"send_resolved\": true\n        \"text\": \"{{ range .Alerts -}}\\r\\n*Severity:* {{ .Annotations.title }}{{ if\n          .Labels.severity }} - `{{ .Labels.severity }}`{{ end }}\\r\\n*Summary:* {{\n          .Annotations.summary }}\\r\\n*Description:* {{ .Annotations.description }}\\r\\n\\r\\n{{\n          end }}\\r\\n\"\n        \"title\": \"{{ define \\\"__alert_severity_prefix_title\\\" -}}\\r\\n    {{ if ne\n          .Status \\\"firing\\\" -}}\\r\\n    :white_check_mark:\\r\\n    {{- else if eq .CommonLabels.severity\n          \\\"critical\\\" -}}\\r\\n    :fire:\\r\\n    {{- else if eq .CommonLabels.severity\n          \\\"warning\\\" -}}\\r\\n    :warning:\\r\\n    {{- else if eq .CommonLabels.severity\n          \\\"info\\\" -}}\\r\\n    :information_source:\\r\\n    {{- else -}}\\r\\n    :question:\\r\\n\n          \\   {{- end }}\\r\\n{{- end }}\\r\\n{{ template \\\"__alert_severity_prefix_title\\\"\n          . }} [{{ .Status | toUpper }}{{ if eq .Status \\\"firing\\\" }} x {{ .Alerts.Firing\n          | len }}{{ end }}] {{ .CommonLabels.alertname }}\\r\\n\"\n    \"route\":\n      \"group_by\":\n      - \"alertname\"\n      \"receiver\": \"slack-host\"\n      \"repeat_interval\": \"12h\"\n      \"routes\":\n      - \"continue\": true\n        \"group_wait\": \"10s\"\n        \"matchers\":\n        - \"type=~junos|mikrotik|cisco\"\n        \"receiver\": \"slack-network\"\n        \"repeat_interval\": \"4h\"\n      - \"continue\": true\n        \"group_wait\": \"10s\"\n        \"matchers\":\n        - \"type=~snapshot|vm|kubernetes|host|hpe-ilo|uptimekuma\"\n        \"receiver\": \"slack-host\"\n        \"repeat_interval\": \"4h\"\n    \"templates\":\n    - \"/etc/alertmanager/*.tmpl\"\n    - \"/tmp/*.tmpl\"\n  \"enabled\": true\n  \"extraSecretMounts\":\n  - \"mountPath\": \"/tmp/\"\n    \"name\": \"slack-template\"\n    \"secretName\": \"slack-template\"\n  \"ingress\":\n    \"annotations\":\n      \"cert-manager.io/cluster-issuer\": \"cluster-ca-issuer\"\n      \"nginx.ingress.kubernetes.io/auth-secret\": \"basic-auth\"\n      \"nginx.ingress.kubernetes.io/auth-type\": \"basic\"\n    \"className\": \"nginx\"\n    \"enabled\": true\n    \"hosts\":\n    - \"host\": \"alertmanager.hostart.az\"\n      \"paths\":\n      - \"path\": \"/\"\n        \"pathType\": \"ImplementationSpecific\"\n    \"labels\":\n      \"kubernetes.io/environment\": \"prod\"\n      \"kubernetes.io/managed-by\": \"Terraform\"\n      \"kubernetes.io/owner\": \"Devops\"\n    \"tls\":\n    - \"hosts\":\n      - \"alertmanager.hostart.az\"\n      \"secretName\": \"alertmanager-tls\"\n\"extraScrapeConfigs\": \"- job_name: git-exporter\\r\\n  static_configs:\\r\\n    - targets:\\r\\n\n  \\       - 10.100.104.70:9100\\r\\n        - 10.100.104.3:9100\\r\\n- job_name: blackbox\\r\\n\n  \\ scrape_interval: 30s\\r\\n  metrics_path: /probe\\r\\n  params:\\r\\n    module: [http_2xx]\n  # Look for a HTTP 200 response.\\r\\n  static_configs:\\r\\n    - targets:\\r\\n        -\n  https://gitlab.hostart.az\\r\\n  relabel_configs:\\r\\n    - source_labels: [__address__]\\r\\n\n  \\     target_label: __param_target\\r\\n    - source_labels: [__param_target]\\r\\n\n  \\     target_label: instance\\r\\n    - target_label: __address__\\r\\n      replacement:\n  prometheus-community-prometheus-blackbox-exporter:9115  # blackbox\\r\\n    - source_labels:\n  [ __meta_dns_name ]\\r\\n      target_label: __param_hostname  # Make domain name\n  become 'Host' header for probe requests\\r\\n    - source_labels: [ __meta_dns_name\n  ]\\r\\n      target_label: vhost  # and store it in 'vhost' label\\r\\n\"\n\"server\":\n  \"ingress\":\n    \"annotations\":\n      \"cert-manager.io/cluster-issuer\": \"cluster-ca-issuer\"\n      \"cert-manager.io/common-name\": \"prometheus.hostart.az\"\n      \"cert-manager.io/subject-organization\": \"hostart.az\"\n      \"nginx.ingress.kubernetes.io/auth-secret\": \"basic-auth\"\n      \"nginx.ingress.kubernetes.io/auth-type\": \"basic\"\n      \"nginx.ingress.kubernetes.io/backend-protocol\": \"HTTP\"\n      \"nginx.ingress.kubernetes.io/force-ssl-redirect\": \"true\"\n    \"enabled\": true\n    \"hosts\":\n    - \"prometheus.hostart.az\"\n    \"tls\":\n    - \"hosts\":\n      - \"prometheus.hostart.az\"\n      \"secretName\": \"prometheus-tls\"\n  \"persistentVolume\":\n    \"enabled\": true\n    \"size\": \"10Gi\"\n    \"storageClass\": \"longhorn\"\n  \"retention\": \"30d\"\n  \"retentionSize\": \"7GB\"\n\"serverFiles\":\n  \"alerting_rules.yml\":\n    \"groups\":\n    - \"name\": \"EmbeddedExporter\"\n      \"rules\":\n      - \"alert\": \"Prod-PrometheusJobMissing\"\n        \"annotations\":\n          \"description\": |-\n            A Prometheus job has disappeared\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus job missing (instance {{ $labels.instance }})\"\n        \"expr\": \"absent(up{job=\\\"prometheus\\\"})\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-PrometheusTargetMissing\"\n        \"annotations\":\n          \"description\": |-\n            A Prometheus target has disappeared. An exporter might be crashed.\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus target missing (instance - {{ $labels.instance }}\n            | {{ $labels.namespace }}/{{ $labels.pod }})\"\n        \"expr\": \"up == 0\"\n        \"for\": \"1m\"\n        \"labels\":\n          \"severity\": \"critical\"\n      - \"alert\": \"Prod-PrometheusAllTargetsMissing\"\n        \"annotations\":\n          \"description\": |-\n            A Prometheus job does not have living target anymore.\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus all targets missing (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"sum by (job) (up) == 0\"\n        \"for\": \"1m\"\n        \"labels\":\n          \"severity\": \"critical\"\n      - \"alert\": \"Prod-PrometheusTargetMissingWithWarmupTime\"\n        \"annotations\":\n          \"description\": |-\n            Allow a job time to start up (10 minutes) before alerting that it's down.\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus target missing with warmup time (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"sum by (instance, job) ((up == 0) * on (instance) group_right(job)\n          (node_time_seconds - node_boot_time_seconds \u003e 600))\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"critical\"\n      - \"alert\": \"Prod-PrometheusConfigurationReloadFailure\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus configuration reload error\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus configuration reload failure (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"prometheus_config_last_reload_successful != 1\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-PrometheusTooManyRestarts\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus has restarted more than twice in the last 15 minutes. It might be crashlooping.\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus too many restarts (instance {{ $labels.instance }})\"\n        \"expr\": \"changes(process_start_time_seconds{job=~\\\"prometheus|pushgateway|alertmanager\\\"}[15m])\n          \u003e 2\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-PrometheusAlertmanagerConfigurationReloadFailure\"\n        \"annotations\":\n          \"description\": |-\n            AlertManager configuration reload error\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus AlertManager configuration reload failure (instance\n            {{ $labels.instance }})\"\n        \"expr\": \"alertmanager_config_last_reload_successful != 1\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-PrometheusAlertmanagerConfigNotSynced\"\n        \"annotations\":\n          \"description\": |-\n            Configurations of AlertManager cluster instances are out of sync\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus AlertManager config not synced (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"count(count_values(\\\"config_hash\\\", alertmanager_config_hash)) \u003e\n          1\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-PrometheusNotConnectedToAlertmanager\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus cannot connect the alertmanager\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus not connected to alertmanager (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"prometheus_notifications_alertmanagers_discovered \u003c 1\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"critical\"\n      - \"alert\": \"Prod-PrometheusRuleEvaluationFailures\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus encountered {{ $value }} rule evaluation failures, leading to potentially ignored alerts.\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus rule evaluation failures (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"increase(prometheus_rule_evaluation_failures_total[3m]) \u003e 0\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"critical\"\n      - \"alert\": \"Prod-PrometheusTemplateTextExpansionFailures\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus encountered {{ $value }} template text expansion failures\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus template text expansion failures (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"increase(prometheus_template_text_expansion_failures_total[3m]) \u003e\n          0\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"critical\"\n      - \"alert\": \"Prod-PrometheusRuleEvaluationSlow\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus rule evaluation took more time than the scheduled interval. It indicates a slower storage backend access or too complex query.\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus rule evaluation slow (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"prometheus_rule_group_last_duration_seconds \u003e prometheus_rule_group_interval_seconds\"\n        \"for\": \"5m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-PrometheusNotificationsBacklog\"\n        \"annotations\":\n          \"description\": |-\n            The Prometheus notification queue has not been empty for 10 minutes\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus notifications backlog (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"min_over_time(prometheus_notifications_queue_length[10m]) \u003e 0\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-PrometheusAlertmanagerNotificationFailing\"\n        \"annotations\":\n          \"description\": |-\n            Alertmanager is failing sending notifications\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus AlertManager notification failing (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"rate(alertmanager_notifications_failed_total[1m]) \u003e 0\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"critical\"\n      - \"alert\": \"Prod-PrometheusTargetEmpty\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus has no target in service discovery\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus target empty (instance {{ $labels.instance }})\"\n        \"expr\": \"prometheus_sd_discovered_targets == 0\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"critical\"\n      - \"alert\": \"Prod-PrometheusTargetScrapingSlow\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus is scraping exporters slowly since it exceeded the requested interval time. Your Prometheus server is under-provisioned.\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus target scraping slow (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"prometheus_target_interval_length_seconds{quantile=\\\"0.9\\\"} / on\n          (interval, instance, job) prometheus_target_interval_length_seconds{quantile=\\\"0.5\\\"}\n          \u003e 1.05\"\n        \"for\": \"5m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-PrometheusLargeScrape\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus has many scrapes that exceed the sample limit\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus large scrape (instance {{ $labels.instance }})\"\n        \"expr\": \"increase(prometheus_target_scrapes_exceeded_sample_limit_total[10m])\n          \u003e 10\"\n        \"for\": \"5m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-PrometheusTargetScrapeDuplicate\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus has many samples rejected due to duplicate timestamps but different values\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus target scrape duplicate (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"increase(prometheus_target_scrapes_sample_duplicate_timestamp_total[5m])\n          \u003e 0\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-PrometheusTsdbCheckpointCreationFailures\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus encountered {{ $value }} checkpoint creation failures\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus TSDB checkpoint creation failures (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"increase(prometheus_tsdb_checkpoint_creations_failed_total[1m]) \u003e\n          0\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"critical\"\n      - \"alert\": \"Prod-PrometheusTsdbCheckpointDeletionFailures\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus encountered {{ $value }} checkpoint deletion failures\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus TSDB checkpoint deletion failures (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"increase(prometheus_tsdb_checkpoint_deletions_failed_total[1m]) \u003e\n          0\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"critical\"\n      - \"alert\": \"Prod-PrometheusTsdbCompactionsFailed\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus encountered {{ $value }} TSDB compactions failures\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus TSDB compactions failed (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"increase(prometheus_tsdb_compactions_failed_total[1m]) \u003e 0\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"critical\"\n      - \"alert\": \"Prod-PrometheusTsdbHeadTruncationsFailed\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus encountered {{ $value }} TSDB head truncation failures\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus TSDB head truncations failed (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"increase(prometheus_tsdb_head_truncations_failed_total[1m]) \u003e 0\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"critical\"\n      - \"alert\": \"Prod-PrometheusTsdbReloadFailures\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus encountered {{ $value }} TSDB reload failures\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus TSDB reload failures (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"increase(prometheus_tsdb_reloads_failures_total[1m]) \u003e 0\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"critical\"\n      - \"alert\": \"Prod-PrometheusTsdbWalCorruptions\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus encountered {{ $value }} TSDB WAL corruptions\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus TSDB WAL corruptions (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"increase(prometheus_tsdb_wal_corruptions_total[1m]) \u003e 0\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"critical\"\n      - \"alert\": \"Prod-PrometheusTsdbWalTruncationsFailed\"\n        \"annotations\":\n          \"description\": |-\n            Prometheus encountered {{ $value }} TSDB WAL truncation failures\n              VALUE = {{ $value }}\n          \"summary\": \"Prometheus TSDB WAL truncations failed (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"increase(prometheus_tsdb_wal_truncations_failed_total[1m]) \u003e 0\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"critical\"\n    - \"name\": \"NodeExporter\"\n      \"rules\":\n      - \"alert\": \"Prod-HostOutOfMemory\"\n        \"annotations\":\n          \"description\": |-\n            Node memory is filling up (\u003c 10% left)\n              VALUE = {{ $value }}\n          \"summary\": \"Host out of memory (instance {{ $labels.instance }})\"\n        \"expr\": \"(node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes * 100\n          \u003c 10) * on(instance) group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"2m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostMemoryUnderMemoryPressure\"\n        \"annotations\":\n          \"description\": |-\n            The node is under heavy memory pressure. High rate of major page faults\n              VALUE = {{ $value }}\n          \"summary\": \"Host memory under memory pressure (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"(rate(node_vmstat_pgmajfault[1m]) \u003e 1000) * on(instance) group_left\n          (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"2m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostUnusualNetworkThroughputIn\"\n        \"annotations\":\n          \"description\": |-\n            Host network interfaces are probably receiving too much data (\u003e 100 MB/s)\n              VALUE = {{ $value }}\n          \"summary\": \"Host unusual network throughput in (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"(sum by (instance) (rate(node_network_receive_bytes_total[2m])) /\n          1024 / 1024 \u003e 100) * on(instance) group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"5m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostUnusualNetworkThroughputOut\"\n        \"annotations\":\n          \"description\": |-\n            Host network interfaces are probably sending too much data (\u003e 100 MB/s)\n              VALUE = {{ $value }}\n          \"summary\": \"Host unusual network throughput out (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"(sum by (instance) (rate(node_network_transmit_bytes_total[2m]))\n          / 1024 / 1024 \u003e 100) * on(instance) group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"5m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostUnusualDiskReadRate\"\n        \"annotations\":\n          \"description\": |-\n            Disk is probably reading too much data (\u003e 100 MB/s)\n              VALUE = {{ $value }}\n          \"summary\": \"Host unusual disk read rate (instance {{ $labels.instance }})\"\n        \"expr\": \"(sum by (instance) (rate(node_disk_read_bytes_total[2m])) / 1024\n          / 1024 \u003e 100) * on(instance) group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"5m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostUnusualDiskWriteRate\"\n        \"annotations\":\n          \"description\": |-\n            Disk is probably writing too much data (\u003e 100 MB/s)\n              VALUE = {{ $value }}\n          \"summary\": \"Host unusual disk write rate (instance {{ $labels.instance }})\"\n        \"expr\": \"(sum by (instance) (rate(node_disk_written_bytes_total[2m])) / 1024\n          / 1024 \u003e 100) * on(instance) group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"2m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostOutOfDiskSpace\"\n        \"annotations\":\n          \"description\": |-\n            Disk is almost full (\u003c 10% left)\n              VALUE = {{ $value }}\n          \"summary\": \"Host out of disk space (instance {{ $labels.instance }})\"\n        \"expr\": \"((node_filesystem_avail_bytes * 100) / node_filesystem_size_bytes\n          \u003c 10 and ON (instance, device, mountpoint) node_filesystem_readonly == 0)\n          * on(instance) group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"2m\"\n        \"labels\":\n          \"severity\": \"critical\"\n      - \"alert\": \"Prod-HostOutOfDiskSpace\"\n        \"annotations\":\n          \"description\": |-\n            Disk is almost full (\u003c 20% left)\n              VALUE = {{ $value }}\n          \"summary\": \"Host out of disk space (instance {{ $labels.instance }})\"\n        \"expr\": \"((node_filesystem_avail_bytes * 100) / node_filesystem_size_bytes\n          \u003c 20 and ON (instance, device, mountpoint) node_filesystem_readonly == 0)\n          * on(instance) group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"2m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostDiskWillFillIn24Hours\"\n        \"annotations\":\n          \"description\": |-\n            Filesystem is predicted to run out of space within the next 24 hours at current write rate\n              VALUE = {{ $value }}\n          \"summary\": \"Host disk will fill in 24 hours (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"((node_filesystem_avail_bytes * 100) / node_filesystem_size_bytes\n          \u003c 10 and ON (instance, device, mountpoint) predict_linear(node_filesystem_avail_bytes{fstype!~\\\"tmpfs\\\"}[1h],\n          24 * 3600) \u003c 0 and ON (instance, device, mountpoint) node_filesystem_readonly\n          == 0) * on(instance) group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"2m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostOutOfInodes\"\n        \"annotations\":\n          \"description\": |-\n            Disk is almost running out of available inodes (\u003c 10% left)\n              VALUE = {{ $value }}\n          \"summary\": \"Host out of inodes (instance {{ $labels.instance }})\"\n        \"expr\": \"(node_filesystem_files_free / node_filesystem_files * 100 \u003c 10 and\n          ON (instance, device, mountpoint) node_filesystem_readonly == 0) * on(instance)\n          group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"2m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostInodesWillFillIn24Hours\"\n        \"annotations\":\n          \"description\": |-\n            Filesystem is predicted to run out of inodes within the next 24 hours at current write rate\n              VALUE = {{ $value }}\n          \"summary\": \"Host inodes will fill in 24 hours (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"(node_filesystem_files_free / node_filesystem_files * 100 \u003c 10 and\n          predict_linear(node_filesystem_files_free[1h], 24 * 3600) \u003c 0 and ON (instance,\n          device, mountpoint) node_filesystem_readonly == 0) * on(instance) group_left\n          (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"2m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostUnusualDiskReadLatency\"\n        \"annotations\":\n          \"description\": |-\n            Disk latency is growing (read operations \u003e 100ms)\n              VALUE = {{ $value }}\n          \"summary\": \"Host unusual disk read latency (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"(rate(node_disk_read_time_seconds_total[1m]) / rate(node_disk_reads_completed_total[1m])\n          \u003e 0.1 and rate(node_disk_reads_completed_total[1m]) \u003e 0) * on(instance)\n          group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"2m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostUnusualDiskWriteLatency\"\n        \"annotations\":\n          \"description\": |-\n            Disk latency is growing (write operations \u003e 100ms)\n              VALUE = {{ $value }}\n          \"summary\": \"Host unusual disk write latency (instance {{ $labels.instance\n            }})\"\n        \"expr\": \"(rate(node_disk_write_time_seconds_total[1m]) / rate(node_disk_writes_completed_total[1m])\n          \u003e 0.1 and rate(node_disk_writes_completed_total[1m]) \u003e 0) * on(instance)\n          group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"2m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostHighCpuLoad-W\"\n        \"annotations\":\n          \"description\": |-\n            CPU load is \u003e 70%\n              VALUE = {{ $value }}\n          \"summary\": \"Host high CPU load (instance {{ $labels.instance }})\"\n        \"expr\": \"(sum by (instance) (avg by (mode, instance) (rate(node_cpu_seconds_total{mode!=\\\"idle\\\"}[2m])))\n          \u003e 0.7) * on(instance) group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"1m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostHighCpuLoad-E\"\n        \"annotations\":\n          \"description\": |-\n            CPU load is \u003e 80%\n              VALUE = {{ $value }}\n          \"summary\": \"Host high CPU load (instance {{ $labels.instance }})\"\n        \"expr\": \"(sum by (instance) (avg by (mode, instance) (rate(node_cpu_seconds_total{mode!=\\\"idle\\\"}[2m])))\n          \u003e 0.8) * on(instance) group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"1m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostHighCpuLoad-C\"\n        \"annotations\":\n          \"description\": |-\n            CPU load is \u003e 90%\n              VALUE = {{ $value }}\n          \"summary\": \"Host high CPU load (instance {{ $labels.instance }})\"\n        \"expr\": \"(sum by (instance) (avg by (mode, instance) (rate(node_cpu_seconds_total{mode!=\\\"idle\\\"}[2m])))\n          \u003e 0.9) * on(instance) group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"1m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostCpuStealNoisyNeighbor\"\n        \"annotations\":\n          \"description\": |-\n            CPU steal is \u003e 10%. A noisy neighbor is killing VM performances or a spot instance may be out of credit.\n              VALUE = {{ $value }}\n          \"summary\": \"Host CPU steal noisy neighbor (instance {{ $labels.nodename\n            }})\"\n        \"expr\": \"(avg by(instance) (rate(node_cpu_seconds_total{mode=\\\"steal\\\"}[5m]))\n          * 100 \u003e 10) * on(instance) group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostCpuHighIowait\"\n        \"annotations\":\n          \"description\": |-\n            CPU iowait \u003e 15%. A high iowait means that you are disk or network bound.\n              VALUE = {{ $value }}\n          \"summary\": \"Host CPU high iowait (instance {{ $labels.nodename }})\"\n        \"expr\": \"(avg by (instance) (rate(node_cpu_seconds_total{mode=\\\"iowait\\\"}[5m]))\n          * 100 \u003e 15) * on(instance) group_left (nodename) node_uname_info{nodename=~\\\".+\\\"}\"\n        \"for\": \"0m\"\n        \"labels\":\n          \"severity\": \"warning\"\n      - \"alert\": \"Prod-HostUnusualDiskIo\"\n        \"annotations\":\n          \"description\": |-\n            Time spent in IO is too high on {{ $labels.instance }}. Check storage for issues.\n              VALUE = {{ $value }}\n          \"summary\": \"Host unusual disk IO (instance {{ $labels.nodename }})\"\n        \"expr\": \"(rate(node_disk